@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:veraenderungOhneRuecksprache a owl:DatatypeProperty,
        owl:ObjectProperty ;
    ns1:kennung "VOR"^^xsd:string ;
    rdfs:domain ns1:AX_BauRaumOderBodenordnungsrecht ;
    rdfs:range ns1:boolean ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "'Veränderung ohne Rücksprache' ist die Kennzeichnung, daß das Kataster- und Vermessungsamt Änderungen an Flurstücken, die am Verfahren, das durch die 'Art der Festlegung' bestimmt ist, beteiligt sind, ohne Rücksprache mit der 'Zuständigen Stelle' durchführen darf."@de .

