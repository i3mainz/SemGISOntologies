@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:postalischeAdresse a owl:ObjectProperty ;
    rdfs:label "postalischeAdresse"@de ;
    ns1:kennung "POA"^^xsd:string ;
    rdfs:domain ns1:AX_GeoreferenzierteGebaeudeadresse ;
    rdfs:range ns1:AX_Post,
        ns1:AX_PostProperty ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition """'Postalische Adresse' enthält Angaben, die entweder direkt geführt oder extern
ergänzt werden. Das Endprodukt muss bei Abgabe an den Kunden diese Informationen
enthalten."""@de .

