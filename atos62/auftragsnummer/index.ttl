@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:auftragsnummer a owl:DatatypeProperty,
        owl:ObjectProperty ;
    ns1:kennung "ANR"@de ;
    rdfs:domain ns1:AX_Benutzungsauftrag,
        ns1:AX_Einrichtungsauftrag,
        ns1:AX_Entsperrauftrag,
        ns1:AX_Fortfuehrungsauftrag,
        ns1:AX_NBAQuittierung,
        ns1:AX_NBAQuittierungsergebnis,
        ns1:AX_NutzerbezogeneBestandsdatenaktualisierung_NBA,
        ns1:AX_Reservierung,
        ns1:AX_Reservierungsauftrag,
        ns1:AX_Reservierungsergebnis,
        ns1:AX_Sperrauftrag ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "Die 'Auftragsnummer' ist eine von der katasterführenden Stelle vergebene eindeutige Kennzeichnung. Alle zu einer Vermessungssache gehörenden Reservierungen müssen dieselbe Auftragsnummer wie der Fortführungsauftrag haben."@de .

