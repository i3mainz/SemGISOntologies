@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:nenner a owl:AnnotationProperty,
        owl:DatatypeProperty,
        owl:ObjectProperty ;
    ns1:kennung "NEN"^^xsd:string ;
    rdfs:domain ns1:AX_Anteil,
        ns1:AX_Flurstuecksnummer ;
    rdfs:range xsd:double,
        xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "Dieses Attribut enthält den Nenner der Flurstücknummer ohne führende Nullen. Diese sind gegebenenfalls bei der Erzeugung des Flurstückskennzeichens zu ergänzen."@de .

