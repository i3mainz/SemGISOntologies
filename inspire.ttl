                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty inspire4:aquifer ;
                                         owl:allValuesFrom inspire4:Aquifer
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty inspire4:aquitard ;
                                         owl:allValuesFrom inspire4:Aquitard
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty inspire4:isLayered ;
                                         owl:allValuesFrom <http://www.opengis.net/gml/3.2#NilReasonType>
                                       ] ,
