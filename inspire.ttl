                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty inspire4:phenomenon ;
                                         owl:allValuesFrom inspire4:AbstractObservableProperty
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty inspire4:sourceObservations ;
                                         owl:allValuesFrom inspire4:ObservationSet
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty inspire4:validTime ;
                                         owl:allValuesFrom <http://www.opengis.net/gml/3.2#TimeInstantProperty>
                                       ] ,
