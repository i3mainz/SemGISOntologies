###  http://www.opengis.net/waterml/2.0#ObservationProcess
<http://www.opengis.net/waterml/2.0#ObservationProcess> rdf:type owl:Class ;
                                                        rdfs:subClassOf <http://www.opengis.net/gml/3.2#AbstractFeature> ,
                                                                        <http://www.opengis.net/waterml/2.0#WaterML2> ,
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty xsd:input ;
                                                                          owl:allValuesFrom <http://www.opengis.net/gml/3.2#Reference>
                                                                        ] ,
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty xsd:operator ;
                                                                          owl:allValuesFrom <http://www.isotc211.org/2005/gmd#CI_ResponsibleParty_Property>
                                                                        ] ,
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty xsd:originatingProcess ;
                                                                          owl:allValuesFrom <http://www.opengis.net/gml/3.2#Reference>
                                                                        ] ,
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty xsd:parameter ;
                                                                          owl:allValuesFrom <http://www.opengis.net/om/2.0#NamedValueProperty>
                                                                        ] ,
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty xsd:processReference ;
                                                                          owl:allValuesFrom <http://www.opengis.net/gml/3.2#Reference>
                                                                        ] ,
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty xsd:processType ;
                                                                          owl:allValuesFrom <http://www.opengis.net/gml/3.2#Reference>
                                                                        ] ,
