@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:zustaendigeStelle a owl:ObjectProperty ;
    rdfs:label "zustaendigeStelle"@de ;
    ns1:kennung "ZST"^^xsd:string ;
    rdfs:domain ns1:AX_Benutzergruppe,
        ns1:AX_BesondererBauwerkspunkt,
        ns1:AX_BesondererGebaeudepunkt,
        ns1:AX_BesondererTopographischerPunkt,
        ns1:AX_Flurstueck,
        ns1:AX_Grenzpunkt,
        ns1:AX_Netzpunkt ;
    rdfs:range ns1:AX_Dienststelle_Schluessel,
        ns1:AX_Dienststelle_SchluesselProperty ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition """'Zuständige Stelle' enthält die verschlüsselte Bezeichnung des Finanzamtes.

Es handelt sich um eine abgeleitete Eigenschaft."""@de .

