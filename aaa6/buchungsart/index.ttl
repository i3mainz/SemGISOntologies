@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:buchungsart a owl:AnnotationProperty,
        owl:DatatypeProperty,
        owl:ObjectProperty ;
    ns1:kennung "BUA"^^xsd:string ;
    rdfs:domain ns1:AX_Buchung_HistorischesFlurstueck,
        ns1:AX_Buchungsstelle,
        ns1:AX_Grundstueck_Bestandsnachweis,
        ns1:AX_K_BUCHUNGSANGABEN,
        ns1:AX_K_Eigentum,
        ns1:AX_K_Eigentum_Anlieger,
        ns1:AX_Recht ;
    rdfs:range ns1:AX_Buchungsart_Buchungsstelle,
        xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "'Buchungsart' bezeichnet die Art der Buchung als langschriftlichen Text."@de .

