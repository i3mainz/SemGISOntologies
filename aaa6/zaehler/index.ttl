@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:zaehler a owl:AnnotationProperty,
        owl:DatatypeProperty,
        owl:ObjectProperty,
        <http://www.w3.org/ns/ldp#Resource>,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    ns1:kennung "ZAE"^^xsd:string ;
    rdfs:domain ns1:AX_Anteil,
        ns1:AX_Flurstuecksnummer ;
    rdfs:range xsd:double,
        xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "Dieses Attribut enthält den Zähler der Flurstücknummer ohne führende Nullen. Diese sind gebenefalls bei der Erzeugung des Flurstückskennzeichens zu ergänzen."@de .

