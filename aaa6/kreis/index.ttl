@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:kreis a owl:ObjectProperty,
        <http://www.w3.org/ns/ldp#Resource>,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    rdfs:label "kreis"@de ;
    ns1:kennung "KRS"^^xsd:string ;
    rdfs:domain ns1:AX_Gebiet_Kreis,
        ns1:AX_Gemeindekennzeichen,
        ns1:AX_GeoreferenzierteGebaeudeadresse,
        ns1:AX_K_FLURSTUECK1,
        ns1:AX_K_FLURSTUECK2,
        ns1:AX_Kreis_Schluessel,
        ns1:AX_Liegenschaftskarte,
        ns1:AX_VerschluesselteLagebezeichnung,
        ns1:AX_Verwaltungsgemeinschaft_Schluessel ;
    rdfs:range ns1:AX_Kreis_Schluessel,
        xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 .

