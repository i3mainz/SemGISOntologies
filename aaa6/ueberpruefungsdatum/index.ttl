@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:ueberpruefungsdatum a owl:DatatypeProperty,
        owl:ObjectProperty ;
    ns1:kennung "PRU"^^xsd:string ;
    rdfs:domain ns1:AX_PunktortAG,
        ns1:AX_PunktortAU,
        ns1:AX_PunktortTA,
        ns1:AX_Schwere ;
    rdfs:range ns1:date ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "'Überprüfungsdatum' gibt das Datum der letzten Überprüfung an, bei der der Schwerewert gegenüber benachbarten Festpunkten als unverändert festgestellt wurde."@de .

