@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:flurnummer a owl:DatatypeProperty,
        owl:ObjectProperty ;
    ns1:kennung "FLN"^^xsd:string ;
    rdfs:domain ns1:AX_Flurstueck,
        ns1:AX_HistorischesFlurstueck,
        ns1:AX_HistorischesFlurstueckALB,
        ns1:AX_HistorischesFlurstueckOhneRaumbezug,
        ns1:AX_K_Anlieger,
        ns1:AX_K_Anliegervermerk,
        ns1:AX_K_Buchungsangaben_HD,
        ns1:AX_K_FLURSTUECK2,
        ns1:AX_K_FLURSTUECK4,
        ns1:AX_Kopf_Flurstueck ;
    rdfs:range ns1:integer,
        xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "'Flurnummer' ist die von der katasterführenden Stelle zur eindeutigen Bezeichnung vergebene Nummer einer Flur, die eine Gruppe von Flurstücken innerhalb einer Gemarkung umfaßt."@de .

