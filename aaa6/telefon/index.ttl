@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:telefon a owl:AnnotationProperty,
        owl:DatatypeProperty,
        owl:ObjectProperty,
        <http://www.w3.org/ns/ldp#Resource>,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    ns1:kennung "TEL"@de ;
    rdfs:domain ns1:AX_Anschrift,
        ns1:AX_K_ANSCHRIFT ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "'Telefon' ist die Nummer des Telefonanschlusses."@de .

