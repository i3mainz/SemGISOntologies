@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:genauigkeitsstufe a owl:AnnotationProperty,
        owl:ObjectProperty,
        <http://www.w3.org/ns/ldp#Resource>,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    ns1:kennung "GST"^^xsd:string ;
    rdfs:domain ns1:AX_DQPunktort,
        ns1:AX_DQSchwere,
        ns1:AX_K_Hoehensystem,
        ns1:AX_K_Lagesystem,
        ns1:AX_K_Positionssystem,
        ns1:AX_K_Schweresystem ;
    rdfs:range ns1:AX_Genauigkeitsstufe_Punktort,
        ns1:AX_Genauigkeitsstufe_Schwere,
        xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "\"Genauigkeitsstufe \" ist die Stufe der Standardabweichung (S) als Ergebnis einer Schätzung (i.d.R. nach der Methode der kleinsten Quadrate), in welche die Messelemente der gleichzeitig berechneten Punkte einbezogen und in der Regel die Ausgangspunkte als fehlerfrei eingeführt wurden."@de .

