        ns1:AX_SchutzgebietNachWasserrecht,
        ns1:AX_SonstigesRecht ;
    rdfs:range ns1:AX_ArtDerFestlegung_AndereFestlegungNachStrassenrecht,
        ns1:AX_ArtDerFestlegung_AndereFestlegungNachWasserrecht,
        ns1:AX_ArtDerFestlegung_BauRaumOderBodenordnungsrecht,
        ns1:AX_ArtDerFestlegung_Denkmalschutzrecht,
