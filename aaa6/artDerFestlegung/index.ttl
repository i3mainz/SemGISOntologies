        ns1:AX_BauRaumOderBodenordnungsrecht,
        ns1:AX_Denkmalschutzrecht,
        ns1:AX_Forstrecht,
        ns1:AX_KlassifizierungNachStrassenrecht,
        ns1:AX_KlassifizierungNachWasserrecht,
        ns1:AX_NaturUmweltOderBodenschutzrecht,
