@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:gehoertZu a owl:ObjectProperty,
        <http://www.w3.org/ns/ldp#Resource>,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    rdfs:label "gehoertZu"@de ;
    ns1:kennung "(INV)11001-12001"^^xsd:string ;
    rdfs:domain ns1:AX_Anschrift,
        ns1:AX_Aufnahmepunkt,
        ns1:AX_Benutzer,
        ns1:AX_Buchungsblattbezirk,
        ns1:AX_Gebaeude,
        ns1:AX_GemarkungsteilFlur,
        ns1:AX_GrablochDerBodenschaetzung,
        ns1:AX_LagebezeichnungMitHausnummer,
        ns1:AX_LagebezeichnungMitPseudonummer,
        ns1:AX_LagebezeichnungOhneHausnummer,
        ns1:AX_Person,
        ns1:AX_Sicherungspunkt,
        ns1:AX_SonstigerVermessungspunkt,
        ns1:AX_SonstigesBauwerkOderSonstigeEinrichtung ;
    rdfs:range ns1:AX_Dienststelle_Schluessel,
        ns1:AX_Dienststelle_SchluesselProperty,
        <http://www.opengis.net/gml/3.2#Reference> ;
    rdfs:subPropertyOf ns1:AAA6 ;
    owl:inverseOf ns1:haengtZusammenMit,
        ns1:inversZu_gehoertZu,
        ns1:istIdentischMitAP,
        ns1:weistAuf ;
    skos:definition """Eine 'Lagebezeichnung ohne Hausnummer' gehört zu einem oder mehreren 'Flurstücken'.

Es handelt sich um die inverse Relationsrichtung."""@de .

