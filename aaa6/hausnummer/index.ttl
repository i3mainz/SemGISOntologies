@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:hausnummer a owl:DatatypeProperty,
        owl:ObjectProperty ;
    ns1:kennung "HNR"^^xsd:string ;
    rdfs:domain ns1:AX_Anschrift,
        ns1:AX_GeoreferenzierteGebaeudeadresse,
        ns1:AX_K_LBEZ_Lagebezeichnung,
        ns1:AX_K_PERS1,
        ns1:AX_LagebezeichnungMitHausnummer ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "'Hausnummer' ist die von der Gemeinde für ein bestehendes oder geplantes Gebäude vergebene Nummer und ggf. einem Adressierungszusatz. Diese Attributart wird in Verbindung mit dem Straßennamen (verschlüsselte oder unverschlüsselte Lagebezeichnung) vergeben."@de .

