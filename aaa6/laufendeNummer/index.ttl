@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:laufendeNummer a owl:AnnotationProperty,
        owl:DatatypeProperty,
        owl:ObjectProperty ;
    ns1:kennung "LFD"@de ;
    rdfs:domain ns1:AX_Buchungsstelle,
        ns1:AX_Fortfuehrungsfall,
        ns1:AX_Fortfuehrungsnummer,
        ns1:AX_K_Fortfuehrungsfall_Ausgabe,
        ns1:AX_K_Fortfuehrungsfall_FM,
        ns1:AX_K_Fortfuehrungsfall_FMJ,
        ns1:AX_LagebezeichnungMitPseudonummer,
        ns1:AX_Lagebezeichnung_K_GEBAEUDE,
        ns1:AX_Namensnummer_K_PERS1 ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "Diese Attributart enthält die laufende Nummer des Fortführungsnachweises."@de .

