@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.adv-online.de/namespaces/adv/gid/6.0#zwischenmarke> a owl:DatatypeProperty,
        owl:ObjectProperty ;
    rdfs:domain <http://www.adv-online.de/namespaces/adv/gid/6.0#AX_Grenzpunkt>,
        <http://www.adv-online.de/namespaces/adv/gid/6.0#AX_K_GRENZPUNKT> ;
    rdfs:range <http://www.adv-online.de/namespaces/adv/gid/6.0#boolean>,
        xsd:boolean,
        xsd:string ;
    rdfs:subPropertyOf <http://www.adv-online.de/namespaces/adv/gid/6.0#AAA6> ;
    skos:definition "'Zwischenmarke' ist eine gebietsweise übliche Bezeichnung für ein Grenzzeichen, das in eine geradlinige Flurstücksgrenze eingebracht ist, um den Grenzverlauf bei fehlender Sichtverbindung oder großer Entfernung zwischen den Grenzzeichen ausreichend erkennbar zu machen." .

