@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:ordnung a owl:AnnotationProperty,
        owl:ObjectProperty ;
    ns1:kennung "ORD"^^xsd:string ;
    rdfs:domain ns1:AX_Hoehenfestpunkt,
        ns1:AX_Klassifikation_Lagefestpunkt,
        ns1:AX_Schwerefestpunkt ;
    rdfs:range ns1:AX_Klassifikation_Ordnung_Lagefestpunkt,
        ns1:AX_Ordnung_Hoehenfestpunkt,
        ns1:AX_Ordnung_Schwerefestpunkt ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition "'Ordnung' gibt eine Klassifikation des HFP an."@de .

