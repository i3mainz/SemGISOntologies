@prefix ns1: <http://www.adv-online.de/namespaces/adv/gid/6.0#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:vermarkung_Marke a owl:ObjectProperty ;
    rdfs:label "vermarkung_Marke"@de ;
    ns1:kennung "VMA"^^xsd:string ;
    rdfs:domain ns1:AX_K_GRENZPUNKT,
        ns1:AX_K_PUNKTE,
        ns1:AX_Netzpunkt ;
    rdfs:range ns1:AX_Marke,
        xsd:string ;
    rdfs:subPropertyOf ns1:AAA6 ;
    skos:definition """Vermarkung (Marke)' ist die Marke zur dauerhaften Kennzeichnung von Vermessungspunkten
im Boden und an baulichen Anlagen."""@de .

