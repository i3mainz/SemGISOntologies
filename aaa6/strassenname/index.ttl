@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.adv-online.de/namespaces/adv/gid/6.0#strassenname> a owl:AnnotationProperty,
        owl:DatatypeProperty,
        owl:ObjectProperty ;
    rdfs:domain <http://www.adv-online.de/namespaces/adv/gid/6.0#AX_Post> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf <http://www.adv-online.de/namespaces/adv/gid/6.0#AAA6> ;
    skos:definition "'Straßenname' enthält die postalisch geführte Schreibweise des Straßennamens, sie kann vom amtlichen Straßennamen abweichen."@de .

