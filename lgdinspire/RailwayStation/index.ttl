@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://linkedgeodata.org/ontology/RailwayStation> a owl:Class,
        <http://www.w3.org/ns/ldp#Resource>,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    rdfs:label "Railway Station"@en ;
    rdfs:subClassOf [ ],
        <http://www.geonames.org/ontology#Feature> ;
    owl:equivalentClass [ ],
        <http://linkedgeodata.org/ontology/TrainStation>,
        <http://schema.org/TrainStation> .

