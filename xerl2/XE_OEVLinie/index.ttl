@prefix ns1: <http://www.xerleben.de/schema/2.0_1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

ns1:XE_OEVLinie a owl:Class,
        owl:ObjectProperty ;
    rdfs:label "XE_OEVLinie"@de ;
    rdfs:range ns1:XE_OEVLinie ;
    rdfs:subClassOf [ ],
        ns1:XE_OrtVonInteresse ;
    rdfs:subPropertyOf ns1:XE_OrtVonInteresse,
        ns1:XErleben2 ;
    skos:definition "Ein OrtVonInteresse der Kategorie OEVLinie ist die planmäßige Route eines öffentlichen Verkehsmittels zwiischen Start- und Endpunkt, beispielsweise die Route der UBahnlinie U5 oder die Buslinie C466."@de,
        "Eine OEVLinie ist die planmäßige Route eines öffentlichen Verkehsmittels zwischen Start- und Endpunkt, beispielsweise die Route der UBahnlinie U5 oder die Buslinie C466."@de ;
    ns1:code "66300000" ;
    ns1:sinceVersion "1.0" .

